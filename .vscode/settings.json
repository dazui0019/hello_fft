{
    "editor.fontSize": 16,
    "editor.fontFamily": "'Cascadia Mono', Consolas, 'Courier New', monospace",
    "actionButtons": {
        "commands": [
            {
                "cwd": "${workspaceFolder}",
                "name": "\ud83e\udd79Rebuild",
                "color": "white",
                "singleInstance": true,
                "saveAll": true,
                "command": ".vscode/Uv4Caller.exe -o ${workspaceFolder}/MDK-ARM/build.log -c 'UV4 -r ${workspaceFolder}/MDK-ARM/hello_fft_nucleo.uvprojx -j0'"
            },
            {
                "cwd": "${workspaceFolder}",
                "name": "\ud83e\udd70Flash",
                "color": "white",
                "singleInstance": true,
                "command": ".vscode/Uv4Caller.exe -o ${workspaceFolder}/MDK-ARM/build.log -c 'UV4 -f ${workspaceFolder}/MDK-ARM/hello_fft_nucleo.uvprojx -j0'"
            },
            {
                "cwd": "${workspaceFolder}",
                "name": "\ud83d\udc25Keil",
                "color": "white",
                "singleInstance": true,
                "command": "UV4 ${workspaceFolder}/MDK-ARM/hello_fft_nucleo.uvprojx"
            },
            {
                "cwd": "${workspaceFolder}",
                "name": "\ud83e\udd14RTT Viewer",
                "color": "white",
                "singleInstance": true,
                "command": "openocd -f ./.vscode/openocd_daplink.cfg -f ./.vscode/openocd_rtt.cfg & telnet localhost 8888"
            },
            {
                "cwd": "${workspaceFolder}",
                "name": "\ud83e\uddd0Monitor",
                "color": "white",
                "singleInstance": true,
                "command": ".vscode/monitor.ps1"
            }
        ],
        "defaultColor": "white",
        "reloadButton": "\ud83e\udee1",
        "loadNpmCommands": false
    },
    "files.associations": {
        "stdint.h": "c",
        "printf.h": "c",
        "gpio.h": "c",
        "driver_ws2812b_basic.h": "c"
    },
    "C_Cpp.default.includePath": [
        "Core/Inc",
        "Drivers/STM32F4xx_HAL_Driver/Inc",
        "Drivers/STM32F4xx_HAL_Driver/Inc/Legacy",
        "Drivers/CMSIS/Device/ST/STM32F4xx/Include",
        "Drivers/CMSIS/Include",
        "Middlewares/ST/ARM/DSP/Inc",
        "Libraries/printf",
        "Libraries/ws2812b/src",
        "Libraries/ws2812b/interface",
        "Libraries/ws2812b/example",
        "Libraries/ws2812b/test",
        "Libraries\\ws28xx"
    ],
    "C_Cpp.default.defines": [
        "USE_HAL_DRIVER",
        "STM32F446xx"
    ],
    "clangd.fallbackFlags": [
        "-I${workspaceFolder}/Core/Inc",
        "-I${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc",
        "-I${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc/Legacy",
        "-I${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32F4xx/Include",
        "-I${workspaceFolder}/Drivers/CMSIS/Include",
        "-I${workspaceFolder}/Middlewares/ST/ARM/DSP/Inc",
        "-I${workspaceFolder}/Libraries/printf",
        "-I${workspaceFolder}/Libraries/ws2812b/src",
        "-I${workspaceFolder}/Libraries/ws2812b/interface",
        "-I${workspaceFolder}/Libraries/ws2812b/example",
        "-I${workspaceFolder}/Libraries/ws2812b/test",
        "-I${workspaceFolder}/Libraries/ws28xx",
        "-DUSE_HAL_DRIVER",
        "-DSTM32F446xx",
        "-D__GNUC__",
        "-target", "arm-none-eabi",
        "--sysroot=D:/Toolchain/arm-gnu-toolchain-14.2.rel1-mingw-w64-x86_64-arm-none-eabi/arm-none-eabi",
    ],
    "cmake.ignoreCMakeListsMissing": true,
    "clangd.path": "D:\\Program Files\\LLVM\\bin\\clangd.exe"
}